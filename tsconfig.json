{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/public/*": ["public/*"],
      "@/components/*": ["components/*"],
      "@/app/*": ["app/*"],
      "@/utils/*": ["utils/*"],
      "@/providers/*": ["providers/*"],
      "@/models/*": ["models/*"],
      "@/constans/*": ["constans/*"],
      "@/layout/*": ["components/layout/*"],
      "@/module/*": ["components/module/*"],
      "@/template/*": ["components/template/*"],
      "@/api/*": ["app/api/*"]
    }
  },
  "types": ["next-auth"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/api/auth/[...nextauth]/route.ts",
    "types/next-auth.d.ts"
  ],
  "exclude": ["node_modules"]
}
